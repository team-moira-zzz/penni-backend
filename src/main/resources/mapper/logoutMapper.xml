<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.moira.pennibackend.domain.logout.mapper.LogoutMapper">
    <!-- 로그아웃 -->
    <update id="updateUserWhenLogout">
        UPDATE USER
        SET
               rtk        = null
             , updated_at = CURRENT_TIMESTAMP
        WHERE  id = #{userId}
    </update>
</mapper>